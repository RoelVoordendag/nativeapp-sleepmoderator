<Page xmlns:navigation="navigation/example-page" loaded="onPageLoaded" navigatedTo="onNavigatedTo" xmlns:gauge="nativescript-pro-ui/gauges" xmlns="http://www.nativescript.org/tns.xsd">
  <StackLayout id="rol">
  <StackLayout id="lol">
  <RadRadialGauge id="gaugeView" margin="10">
                <RadRadialGauge.scales>
                <RadialScale startAngle="0" sweepAngle="360" minimum="0" maximum="100" radius="0.9">
                    <RadialScale.scaleStyle>
                        <ScaleStyle ticksVisible="false" labelsVisible="false" lineThickness="0" />
                            </RadialScale.scaleStyle>
                                <RadialScale.indicators>  
                                    <RadialBarIndicator minimum="0" maximum="100" location="1">
                                        <RadialBarIndicator.indicatorStyle>
                                            <BarIndicatorStyle fillColor="rgba(132,235,247,0.5)" barWidth="0.4"/>
                                        </RadialBarIndicator.indicatorStyle>
                                    </RadialBarIndicator>
                                        <!-- <label text="lool"></label> -->
                                    <RadialBarIndicator minimum="0" maximum="0"  location="1" isAnimated="true">
                                        <RadialBarIndicator.indicatorStyle>
                                            <BarIndicatorStyle cap="Round" fillColor="rgba(132,235,247,1)" barWidth="0.4"/>
                                        </RadialBarIndicator.indicatorStyle>
                                    </RadialBarIndicator>
                                </RadialScale.indicators>
                            </RadialScale>
                        </RadRadialGauge.scales>
                    </RadRadialGauge>
                    </StackLayout>
                    <StackLayout id="lmao">
                     <RadRadialGauge id="yow" margin="10">
                <RadRadialGauge.scales>
                <RadialScale startAngle="0" sweepAngle="360" minimum="0" maximum="100" radius="0.9">
                    <RadialScale.scaleStyle>
                        <ScaleStyle ticksVisible="false" labelsVisible="false" lineThickness="0" />
                            </RadialScale.scaleStyle>
                                <RadialScale.indicators>  
                                    <RadialBarIndicator minimum="0" maximum="100" location="1">
                                        <RadialBarIndicator.indicatorStyle>
                                            <BarIndicatorStyle fillColor="rgba(132,235,247,0.5)" barWidth="0.4"/>
                                        </RadialBarIndicator.indicatorStyle>
                                    </RadialBarIndicator>
                                        <!-- <label text="lool"></label> -->
                                    <RadialBarIndicator minimum="0" maximum="0"  location="1" isAnimated="true">
                                        <RadialBarIndicator.indicatorStyle>
                                            <BarIndicatorStyle cap="Round" fillColor="rgba(132,235,247,1)" barWidth="0.4"/>
                                        </RadialBarIndicator.indicatorStyle>
                                    </RadialBarIndicator>
                                </RadialScale.indicators>
                            </RadialScale>
                        </RadRadialGauge.scales>
                    </RadRadialGauge>
                    </StackLayout>
                    </StackLayout>
    <!-- << gauges-indicators-add-bars-xml -->
</Page>